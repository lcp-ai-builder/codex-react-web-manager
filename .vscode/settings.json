{
  "cSpell.enabled": false,
  "editor.fontFamily": "'Google Sans Code','Monaspace Neon',Monaco,'JetBrains Mono','WenQuanYi Micro Hei Mono','Microsoft YaHei','Fira Code', Consolas, 'Courier New', monospace",
  "editor.wordWrap": "wordWrapColumn",
  "editor.wordWrapColumn": 200,
  "eslint.enable": true,
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    ".config": ".prettierrc*,eslint*,.eslintignore,.eslint*,.gitignore,jsconfig.json,tsconfig.json,vite.config.js,package*,.env*",
    ".other": "description,note,AGENTS.md",
    ".shell": "*.sh",
    ".static": "favicon.*,index.html"
  },
  "explorer.sortOrder": "type",
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.idea": true,
    "**/.svn": true,
    "**/Thumbs.db": true,
    "node_modules": true,
    "package-lock.json": true,
    "yarn.lock": true
  },
  "git.enableCommitSigning": true,
  "prettier.printWidth": 200,
  "vite.autoStart": false,
  "workbench.colorTheme": "Cursor Dark",
  "workbench.iconTheme": "vs-seti"
}